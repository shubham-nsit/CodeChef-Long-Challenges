#include <stdio.h>
#include <math.h>

int main(void) {
	// your code goes here
	int t,n,k,i,count=0;
	scanf("%d",&t);
	while(t--)
	{
	    scanf("%d %d",&n,&k);
	    int p[n];
	    count=0;
	    for(i=0;i<n;i++)
	    {
	        scanf("%d",&p[i]);
	        
	        if(p[i]>k)
	        {
	            count=count+(p[i]-k);
	        }
	    }
	    printf("%d\n",count);
	}
	return 0;
}

