#include <stdio.h>

int main(void) {
	// your code goes here
	int t;
	scanf("%d",&t);
	while(t--)
	{
	    int n;
	    long int k;
	    scanf("%d %ld",&n,&k);
	    long int d,i;
	    for(i=0;i<n;i++)
	    {
	        scanf("%ld",&d);
	        if(d%k==0)
	        {
	            printf("1");
	        }
	        else
	        {
	            printf("0");
	        }
	        
	    }
	    printf("\n");
	}
	return 0;
}

