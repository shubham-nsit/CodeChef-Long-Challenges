#include <iostream>
#include <string>
#include <boost/algorithm/string.hpp>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        string data;
        cin>>data;
        // Replace all occurrences of 'LIB' with 'XXX'
        // Case Sensitive Version
        boost::replace_all(data, "party", "pawri");
        cout<<data<<endl;

    }        
}


