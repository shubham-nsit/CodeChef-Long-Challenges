#include<bits/stdc++.h>
using namespace std;

int main()
{
    long int t;
    cin>>t;
    while(t--)
    {
        int w_1,w_2,x_1,x_2,m;
        cin>>w_1>>w_2>>x_1>>x_2>>m;
        int diff=w_2-w_1;
        int max=x_2*m;
        int min=x_1*m;
        if(diff>=min && diff<=max)
        {
            cout<<"1\n";
        }
        else
        {
            cout<<"0\n";
        }
        
    }
    return 0;
}
